    },
    "CapacitorSQLite": {
      "iosDatabaseLocation": "Library",
      "iosIsEncryption": true,
      "iosKeychainPrefix": "angular-sqlite-app-starter",
      "iosBiometric": {
        "biometricAuth": false,
        "biometricTitle" : "Biometric login for capacitor sqlite"
      },
      "androidIsEncryption": true,
      "androidBiometric": {
